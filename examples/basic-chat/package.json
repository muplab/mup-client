{
  "name": "mup-basic-chat-example",
  "version": "1.0.0",
  "description": "Basic chat application using MUP SDK",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "ts-node server.ts",
    "client": "serve -s . -p 3001",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@muprotocol/client": "^1.0.0",
    "@muprotocol/server": "^1.0.0",
    "@muprotocol/types": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.0",
    "serve": "^14.2.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}